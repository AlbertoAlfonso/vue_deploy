AA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,YAAA,GAAY,KAAd;AAAA,KAAA;AAAkD,GAA5I,E,iBAAmH,IAAA,CAAA,e,CAAnH,EAAkI,EAAlI,EAAkI,UAAlI,CAAiJ,CAAvK,CA6BI,EA3BJ,UA2BI,EA1BJ,WA0BI,EAzBJ,mBAAA,CAwBM,KAxBN,EAAA,WAAA,EAwBM,CAvBJ,mBAAA,CAsBM,KAtBN,EAAA,WAAA,EAsBM,C,WAAA,EApBJ,WAoBI,EAlBJ,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFJ,mBAAA,CAA8D,KAA9D,EAAA,WAAA,EAA4B,aAAQ,gBAAA,CAAG,IAAA,CAAA,cAAH,CAApC,EAAqD,CAArD,CAEI,EADJ,mBAAA,CAAkE,KAAlE,EAAA,WAAA,EAA4B,iBAAY,gBAAA,CAAG,IAAA,CAAA,cAAH,CAAxC,EAAyD,CAAzD,CACI,CAHN,CAkBI,EAbJ,WAaI,EAZJ,mBAAA,CAMM,KANN,EAAA,WAAA,EAMM,CALK,IAAA,CAAA,OAAA,IAAW,IAAA,CAAA,OAAA,CAAQ,M,kBAA5B,mBAAA,CAII,GAJJ,EAII,WAJJ,EAII,E,kBAHJ,mBAAA,CAEK,SAFL,EAEK,IAFL,EAEK,WAAA,CAFgB,IAAA,CAAA,OAEhB,EAFuB,UAAjB,MAAiB,EAAX;yBAAjB,mBAAA,CAEK,IAFL,EAEK,IAFL,EAEK,CADH,mBAAA,CAAgE,KAAhE,EAAA,WAAA,EAA4B,aAAQ,gBAAA,CAAG,MAAM,CAAC,OAAP,EAAH,CAApC,EAAqD,CAArD,CACG,CAFL,C;GAEK,CAFL,E,GAAA,CAGI,EAJJ,C,iCAKI,CANN,CAYI,EAJJ,WAII,EAHJ,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADJ,mBAAA,CAA+D,KAA/D,EAAA,WAAA,EAA4B,aAAQ,gBAAA,CAAG,IAAA,CAAA,cAAH,CAApC,EAAqD,CAArD,CACI,CAFN,CAGI,CAtBN,CAuBI,CAxBN,CAyBI,CArCN,C","sourcesContent":["<template>\r\n  <div class=\"bootstrap\">\r\n    <h1>{{ msg }}</h1>\r\n    <h5>REST service call are easy to do with Vue.js, if you know how to do it.</h5>\r\n    <p></p>\r\n    <h6><span class=\"badge bg-primary\"> LetÂ´s go!</span> Call a Spring Boot REST backend service, by clicking a button:</h6>\r\n    <p></p>\r\n      <button class=\"btn btn-success\" @click=\"callHelloApi()\" id=\"btnCallHello\">/hello (GET)</button>\r\n      <p></p>\r\n    <h4>Backend response: <span class=\"alert alert-primary\" role=\"alert\" :show=\"showResponse\" dismissible @dismissed=\"showResponse=false\">{{ backendResponse }}</span></h4>\r\n\r\n    <button class=\"btn btn-secondary\" data-bs-toggle=\"collapse\" data-bs-target=\"#collapseOuter\">Show Response details</button>\r\n    <p></p>\r\n    <div class=\"collapse\" id=\"collapseOuter\">\r\n      <div class=\"card card-body\">\r\n        The Response hat this details\r\n        <button class=\"btn btn-primary\" data-bs-toggle=\"collapse\" data-bs-target=\"#collapseInnerStatusCode\">HTTP Status</button>\r\n\r\n        <div class=\"collapse\" id=\"collapseInnerStatusCode\">\r\n          <div class=\"card card-body\">Status: {{ httpStatusCode }}</div>\r\n          <div class=\"card card-body\">Statustext: {{ httpStatusText }}</div>\r\n        </div>\r\n\r\n        <button class=\"btn btn-warning\" data-bs-toggle=\"collapse\" data-bs-target=\"#collapseInnerHeaders\">HTTP Headers</button>\r\n        <div class=\"collapse\" id=\"collapseInnerHeaders\">\r\n          <p v-if=\"headers && headers.length\">\r\n          <li v-for=\"header of headers\">\r\n            <div class=\"card card-body\">Header: {{ header.valueOf() }}</div>\r\n          </li>\r\n          </p>\r\n        </div>\r\n\r\n        <button class=\"btn btn-danger\" data-bs-toggle=\"collapse\" data-bs-target=\"#collapseInnerResponseConfig\">Full Request configuration</button>\r\n        <div class=\"collapse\" id=\"collapseInnerResponseConfig\">\r\n          <div class=\"card card-body\">Config: {{ responseConfig }} </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { defineComponent } from 'vue';\r\nimport api from '../api/backend-api'\r\nimport {AxiosError, AxiosRequestConfig} from \"axios\";\r\n\r\ninterface State {\r\n  msg: string;\r\n  showResponse: boolean;\r\n  backendResponse: string;\r\n  responseConfig: any;\r\n  httpStatusCode: number;\r\n  httpStatusText: string;\r\n  headers: string[];\r\n  errors: AxiosError[]\r\n}\r\n\r\nexport default defineComponent({\r\n  name: 'Bootstrap',\r\n\r\n  data: (): State => {\r\n    return {\r\n      msg: 'Nice Bootstrap candy!',\r\n      showResponse: false,\r\n      backendResponse: '',\r\n      responseConfig: '',\r\n      httpStatusCode: 0,\r\n      httpStatusText: '',\r\n      headers: ['Noting here atm. Did you call the Service?'],\r\n      errors: []\r\n    }\r\n  },\r\n  methods: {\r\n    callHelloApi (): any {\r\n        api.hello().then(response => {\r\n          this.backendResponse = response.data;\r\n          this.httpStatusCode = response.status;\r\n          this.httpStatusText = response.statusText;\r\n          this.headers = response.headers;\r\n          this.responseConfig = response.config;\r\n          this.showResponse=true\r\n        })\r\n        .catch((error: AxiosError) => {\r\n          this.errors.push(error)\r\n        })\r\n    }\r\n  }\r\n});\r\n\r\n</script>\r\n\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\np {\r\n  margin-bottom: 20px;\r\n}\r\n\r\nh1, h2 {\r\n  font-weight: normal;\r\n}\r\n\r\nul {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\nli {\r\n  display: inline-block;\r\n  margin: 0 10px;\r\n}\r\n\r\na {\r\n  color: #42b983;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}