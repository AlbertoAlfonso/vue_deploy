AAyD;;aAAzC,IAAA,CAAA,IAAA,CAAK,SAAL,GAAc,M;MAA2B;AAAzB,IAAA,WAAW,EAAC;AAAa,GAArE,E,IAAA,E;;AAAA,G,iBAA4B,IAAA,CAAA,IAAA,CAAK,S,GAU7B,E,gBATJ,mBAAA,CAAmE,OAAnE,EAAmE;AAA5D,IAAA,IAAI,EAAC,MAAuD;;aAAvC,IAAA,CAAA,IAAA,CAAK,QAAL,GAAa,M;MAA0B;AAAxB,IAAA,WAAW,EAAC;AAAY,GAAnE,E,IAAA,E;;AAAA,G,iBAA4B,IAAA,CAAA,IAAA,CAAK,Q,GAS7B,EAPJ,mBAAA,CAAqD,QAArD,EAAqD;AAA5C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,aAAA,EAAF;AAAA,KAAA;AAAuC,GAArD,EAAiC,aAAjC,CAOI,EALO,IAAA,CAAA,Y,kBAAX,mBAAA,CAA2E,KAA3E,EAA2E,UAA3E,EAA2E,CAAlD,mBAAA,CAA4C,IAA5C,EAA4C,IAA5C,EAAI,2BAAsB,gBAAA,CAAG,IAAA,CAAA,IAAA,CAAK,EAAR,CAA1B,EAAoC;AAAA;AAApC,GAAkD,CAA3E,C,qCAKI,EAHU,IAAA,CAAA,Y,kBAAd,mBAAA,CAAyG,QAAzG,EAAyG;UAAA;AAA5E,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,IAAA,CAAA,YAAA,EAAF;AAAA,KAAA;AAAuE,GAAzG,EAAoD,mBAAc,gBAAA,CAAE,IAAA,CAAA,IAAA,CAAK,EAAP,CAAd,GAAyB,qBAA7E,EAAgG;AAAA;AAAhG,G,qCAGI,EADM,IAAA,CAAA,iB,kBAAV,mBAAA,CAAuG,IAAvG,EAAuG,UAAvG,EAA6B,oBAAe,gBAAA,CAAE,IAAA,CAAA,aAAA,CAAc,SAAhB,CAAf,GAA0C,GAA1C,GAA2C,gBAAA,CAAE,IAAA,CAAA,aAAA,CAAc,QAAhB,CAAxE,EAAgG;AAAA;AAAhG,G,qCACI,CAfN,C","sourcesContent":["<template>\r\n  <div class=\"user\">\r\n    <h1>Create User</h1>\r\n\r\n    <h3>Just some database interaction...</h3>\r\n\r\n    <input type=\"text\" v-model=\"user.firstName\" placeholder=\"first name\">\r\n    <input type=\"text\" v-model=\"user.lastName\" placeholder=\"last name\">\r\n\r\n    <button @click=\"createNewUser()\">Create User</button>\r\n\r\n    <div v-if=\"showResponse\"><h6>User created with Id: {{ user.id }}</h6></div>\r\n\r\n    <button v-if=\"showResponse\" @click=\"retrieveUser()\">Retrieve user {{user.id}} data from database</button>\r\n\r\n    <h4 v-if=\"showRetrievedUser\">Retrieved User {{retrievedUser.firstName}} {{retrievedUser.lastName}}</h4>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport { defineComponent } from 'vue';\r\nimport api from \"../api/backend-api\";\r\nimport {AxiosError} from \"axios\";\r\n\r\ninterface State {\r\n  user: {\r\n    id: number\r\n    firstName: string,\r\n    lastName: string;\r\n  };\r\n  retrievedUser: {\r\n    id: number\r\n    firstName: string,\r\n    lastName: string;\r\n  };\r\n  showResponse: boolean;\r\n  showRetrievedUser: boolean;\r\n  errors: AxiosError[]\r\n}\r\n\r\nexport default defineComponent({\r\n  name: 'User',\r\n\r\n  data: (): State => {\r\n    return {\r\n      errors: [],\r\n      user: {\r\n        id: 0,\r\n        firstName: '',\r\n        lastName: ''\r\n      },\r\n      showResponse: false,\r\n      retrievedUser: {\r\n        id: 0,\r\n        firstName: '',\r\n        lastName: ''\r\n      },\r\n      showRetrievedUser: false\r\n    }\r\n  },\r\n  methods: {\r\n    // Fetches posts when the view is created.\r\n    createNewUser () {\r\n      api.createUser(this.user.firstName, this.user.lastName).then(response => {\r\n          // JSON responses are automatically parsed.\r\n          this.user.id = response.data;\r\n          console.log('Created new User with Id ' + response.data);\r\n          this.showResponse = true\r\n        })\r\n        .catch(e => {\r\n          this.errors.push(e)\r\n        })\r\n    },\r\n    retrieveUser () {\r\n      api.getUser(this.user.id).then(response => {\r\n          // JSON responses are automatically parsed.\r\n          this.retrievedUser = response.data;\r\n          this.showRetrievedUser = true\r\n        })\r\n        .catch((error: AxiosError):void => {\r\n          this.errors.push(error)\r\n        })\r\n    }\r\n  }\r\n});\r\n</script>\r\n\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n  h1, h2 {\r\n    font-weight: normal;\r\n  }\r\n\r\n  ul {\r\n    list-style-type: none;\r\n    padding: 0;\r\n  }\r\n\r\n  li {\r\n    display: inline-block;\r\n    margin: 0 10px;\r\n  }\r\n\r\n  a {\r\n    color: #42b983;\r\n  }\r\n</style>\r\n"],"sourceRoot":""}]}